// To start the dev server

$ concurrently --kill-others "ng serve" "nodemon server.js"

// build for production
$ ng build --prod

// Deploy process
// from ~gregl
$ ./mypkg.sh mytrainingdocs-ws

// from ~gregl/mytrainingdocs-ws
// Enter passphrase for key '/Users/gregorylayer/.ssh/id_rsa' ... 'cyrano1'
$ sftp gregl@mytrainingdocs.com

// login to mytrainingdocs using
$ ssh gregl@mytrainingdocs.com

// once on mytrainingdocs.com run the ~gregl/deploy script
$ ./deploy